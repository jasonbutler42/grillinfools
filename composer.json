{
  "name": "jasonbutler42/grillinfools",
  "type": "project",
  "license": "MIT",
  "description": "WordPress stack for grillinfools.com",
  "homepage": "http://grillinfools.com",
  "authors": [
    {
      "name": "Jason Butler",
      "email": "jasonbutler42@gmail.com",
      "homepage": "http://jasonbutler42.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "generate-salts": true
  },
  "autoload": {
    "psr-0": {"Bedrock\\Installer": "scripts"}
  },
  "scripts": {
    "post-root-package-install": ["Bedrock\\Installer::addSalts"]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "ooyala/ooyala-wordpress-plugin",
        "version": "1.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "tar",
          "url": "http://www.ravendevelopers.com/sites/default/files/assets/attachments/1/ooyala-for-wp.tar_0.gz"
        },
        "require" : {
          "composer/installers": "v1.0.12"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "version": "4.1",
        "type": "webroot",
        "dist": {
          "type": "zip",
          "url": "https://wordpress.org/wordpress-4.1.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "wordpress/wordpress": "4.1",
    "fancyguy/webroot-installer": "1.1.0",
    "composer/installers": "v1.0.12",
    "vlucas/phpdotenv": "~1.0.6",
    "wpackagist-plugin/ad-injection": "1.2.0.18",
    "wpackagist-plugin/advanced-responsive-video-embedder": "5.1.1",
    "wpackagist-plugin/akismet": "3.0.4",
    "wpackagist-plugin/all-in-one-seo-pack": "2.2.5.1",
    "wpackagist-plugin/all-in-one-wp-security-and-firewall": "v3.8.9",
    "wpackagist-plugin/ap-honeypot": "1.4",
    "wpackagist-plugin/askapache-debug-viewer": "3.1",
    "wpackagist-plugin/broken-link-checker": "1.10.4",
    "wpackagist-plugin/business-directory-plugin": "3.5.5",
    "wpackagist-plugin/co-authors-plus": "3.1.1",
    "wpackagist-plugin/easyrecipe": "3.2.2885",
    "wpackagist-plugin/floating-social-bar": "1.1.5",
    "wpackagist-plugin/go-newrelic": "0.3",
    "wpackagist-plugin/honeypot-comments": "1.1.0",
    "wpackagist-plugin/infinite-scroll": "2.6.2",
    "wpackagist-plugin/iq-block-country": "1.1.16",
    "wpackagist-plugin/jetpack": "3.3",
    "ooyala/ooyala-wordpress-plugin": "1.0",
    "wpackagist-plugin/page-links-to": "2.9.4",
    "wpackagist-plugin/raw-html": "1.4.14",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.4",
    "wpackagist-plugin/remove-image-links": "1.2",
    "wpackagist-plugin/sabre": "1.2.2",
    "roots/soil": "3.0.1",
    "wpackagist-plugin/subscribe-to-comments-reloaded": "150207",
    "wpackagist-plugin/search-and-replace": "2.7.0",
    "wpackagist-plugin/nofollow": "1.4.2",
    "wpackagist-plugin/ultimate-posts-widget": "2.0.3",
    "wpackagist-plugin/w3-total-cache": "0.9.4.1",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "wpackagist-plugin/wp-instagram-widget": "1.3.1",
    "wpackagist-plugin/wp-recent-posts-from-category": "1.1.0",
    "wpackagist-plugin/wp-smushit": "1.7",
    "wpackagist-plugin/wp-stripe": "1.5",
    "wpackagist-plugin/wp-stripe-email-receipts": "1.0",
    "wpackagist-plugin/yet-another-related-posts-plugin": "4.2.4"
  },
  "extra": {
    "installer-paths": {
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "web/wp",
    "webroot-package": "wordpress/wordpress"
  }
}
